{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "Mirage.csproj",
                        "Mirage.Authenticators.csproj",
                        "Mirage.Components.csproj"
                    ],
                    "src": "../"
                }
            ],
            "dest": "api",
            "disableDefaultFilter": false
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "**/*.md",
                    "**/toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**",
                    "**.png",
                    "**.jpg",
                    "**.svg"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_homepage": "https://github.com/MirageNet/Mirage",
            "_appLogoPath": "logo.png",
            "_enableSearch": true,
            "_gitContribute": {
                "branch": "master"
            }
        },
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/unity"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false
    }
}